{
      "name": "mono-node-kit-ts",
      "version": "1.1.0",
      "description": "A express server starter from scratch using Typescript, Jest",
      "bin": "./bin/cli.js",
      "scripts": {
            "start": "NODE_ENV=production  ts-node index.ts",
            "start:dev": "NODE_ENV=development  nodemon --exec ts-node index.ts",
            "build": "tsc",
            "prebuild": "rm -rf dist/*",
            "test": "NODE_ENV=test  jest",
            "test:watch": "NODE_ENV=test  jest  --watchAll --runInBand --detectOpenHandles",
            "test:cov": "NODE_ENV=test  jest --coverage --runInBand --detectOpenHandles",
            "postinstall": "npx husky install",
            "format": "prettier --write ."
      },
      "author": "heaty566 <heaty566@gmail.com>",
      "license": "MIT",
      "devDependencies": {
            "@types/compression": "^1.7.0",
            "@types/cors": "^2.8.8",
            "@types/dotenv": "^8.2.0",
            "@types/express": "^4.17.8",
            "@types/helmet": "^4.0.0",
            "@types/jest": "^26.0.15",
            "@types/morgan": "^1.9.1",
            "@types/node": "^14.14.5",
            "@types/winston": "^2.4.4",
            "husky": "^7.0.0",
            "jest": "^26.6.1",
            "nodemon": "^2.0.6",
            "prettier": "^2.4.1",
            "supertest": "^5.0.0",
            "ts-jest": "^26.4.3",
            "ts-node": "^9.0.0",
            "tslint": "^5.12.1",
            "typescript": "^4.0.5"
      },
      "dependencies": {
            "compression": "^1.7.4",
            "cors": "^2.8.5",
            "dotenv": "10.0.0",
            "express": "^4.17.1",
            "express-session": "^1.17.1",
            "helmet": "^4.1.1",
            "morgan": "^1.10.0",
            "winston": "^3.3.3"
      },
      "repository": "https://github.com/MonoInfinity/node-kit-ts.git"
}
